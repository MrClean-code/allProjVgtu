<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="612.0" prefWidth="569.0" spacing="20.0" style="-fx-background-color: red;"
      xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.task2.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Button fx:id="buttonStart1" onAction="#onHelloButtonClick" text="Draw Shape"/>
    <TextField fx:id="textField2" prefHeight="25.0" prefWidth="431.0"/>
    <ColorPicker fx:id="colorPicker" prefHeight="42.0" prefWidth="109.0"/>
    <Canvas fx:id="canvas" height="200.0" style="-fx-blend-mode: red;" width="200.0"/>
    <Button fx:id="delete" mnemonicParsing="false" onAction="#delete" text="Delete"/>
    <Label fx:id="text" text=""/>
    <Label fx:id="welcomeText" text="Square"/>
    <TextField fx:id="textField" prefHeight="25.0" prefWidth="431.0"/>
</VBox>
